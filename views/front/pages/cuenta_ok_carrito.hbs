                                    <div class="modularGrid-module u-xs-25">
                                        {{>pricePanel2-promo}}
                                    </div><!-- mod -->

                                    <div class="modularGrid-module u-xs-25">
                                        {{>pricePanel3-promo}}
                                    </div><!-- mod -->

                                    <div class="modularGrid-module u-xs-25">
                                        {{>pricePanel4-promo}}
                                    </div><!-- mod -->

                                </div><!-- grid -->

                            </div><!-- mod -->

                        </div><!-- grid -->
                        <button class="button button button--big u-elem-center">
                            <span>+info</span>
                        </button>
                    </section>

                    <a class="modularGrid-module u-xs-80 ventaCMS-link u-txr" href="">Continuar navegando</a>
