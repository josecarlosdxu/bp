                <th class="table-th table-item u-xs-20">Total<i class="icon icon-sort fa fa-caret-down"></i></th>
              </tr>
              <tr class="table-tr  table-tr--filters">
                <th class="table-th table-item">
                  <input type="text" class="form-input form-input--collapse">
                </th>
                <th class="table-th table-item">
                  <input type="text" class="form-input form-input--collapse">
                </th>
                <th class="table-th table-item">
                  <select name="" id="" class="form-select form-select--collapse">
                    <option value="">--</option>
                  </select>
                </th>
                <th class="table-th table-item">
                  <input type="text" class="form-input form-input--collapse">
                </th>
              </tr>
            </thead>
            <tbody class="table-tbody  table-tbody--stripped">
              <tr class="table-tr" data-js-back="row">
                <td class="table-item">DEV0728934</td>
                <td class="table-item ">17/04/2015</td>
                <td class="table-item">En revisión</td>
                <td class="table-item Blue">299€</td>
              </tr>
              <tr class="table-tr" data-js-back="row">
                <td class="table-item">DEV0728934</td>
                <td class="table-item ">17/04/2015</td>
                <td class="table-item">Resuelto</td>
                <td class="table-item Blue">123€</td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
