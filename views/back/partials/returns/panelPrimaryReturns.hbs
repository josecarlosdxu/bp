<div class="panel panel--active" data-js-panel-name="panel01" data-js-panel-state="active">
  <div class="panel-title">
    <svg class="icon icon--inline icon--xs" role="img">
      <use xlink:href="../../img/icons/svg-defs.svg#icon-wallet"/></use>
    </svg>
    <div class="panel-titleName">
      <a class="panel-breadcrumbLink" href="">Devoluciones</a>
    </div>
  </div>
  <div class="panel-content">
    <section class="panel-section">
      <div class="modularGrid">
        <div class="modularGrid-module u-pdl--half u-pdt">
          <svg class="icon icon--inline icon--xs" role="img"><use xlink:href="../../img/icons/svg-defs.svg#icon-mydata"></use></svg>
          <h3 class="form-fieldset-title u-iblock">
            Borrador de devolución
          </h3 >
          <span class="form-info" data-js-tooltip="tooltip_pack"><i class="icon fa-lg fa-info-circle"></i></span>
          <button data-js-actiongroup-download="" class="button button--big u-block u-pdl">Crear borrador</button>
        </div>
      </div>
      <div class="modularGrid u-pdb">
        <div class="modularGrid-module u-pdl--half u-pdt2">
