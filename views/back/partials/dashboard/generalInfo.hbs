
<div class="modularGrid">
    <div class="modularGrid-module u-xs-20">
        <div class="u-pdr">
            <article data-js-id_row="11132" class="u-brd">
                <div class="adressCard-title adressCard-title--flexcontent">
                    {{>svgIcon type='wallet' size='md' class='u-op-60 u-pdr--half'}}
                    <h1 class="adressCard-titleName">Monedero</h1>
                </div>
                <div class="adressCard-content adressCard-content--flexcontent">
                    <p><strong>Saldo actual:</strong> 1250,60 €</p>
                </div>
            </article>
        </div>
    </div>
    <div class="modularGrid-module u-xs-20">
        <div class="u-pdr">
            <article data-js-id_row="11132" class="u-brd">
                <div class="adressCard-title adressCard-title--flexcontent">
                    {{>svgIcon type='packUpgrade' size='md' class='u-op-60 u-pdr--half'}}
