	<div class="productSheet" data-js-customer_group="" data-js-stock_quantity="0" data-js-product_attribute="0" data-js-id_product="7452" data-ng-controller="ProductDetailCtrl" >		
		{{>imgBoxList}}

		<section class="productSheet-details">
			<div class="productSheet-features">
				{{>productSheetHeader}}
				{{>productSheetAdvantages}}							
