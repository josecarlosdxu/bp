											<option value="774"> 5 / 5.5 Kg. </option>
											<option value="770"> 4.5 / 5 Kg. </option>
										</select>
									
										<label class="form-label">Lote de 4 Embutidos</label>
										<select name="Lote de 4 Embutidos" data-js-select_attributes="152" class="form-select">
											<option value="1007" selected="selected"> Sin Embutidos </option>
											<option value="1006"> Con Embutidos </option>
										</select>										
									</div>
								</div>		
						{{>productSheetDiscountBadge}}
						{{>productSheetActions}}
					</div>
